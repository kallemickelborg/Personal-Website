    "html-react-parser": "^5.1.10",
    "next": "^14.2.4",
    "next-sitemap": "^4.2.3",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.4.1",
    "use-scramble": "^2.2.15"
